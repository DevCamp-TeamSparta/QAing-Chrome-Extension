{
	"name": "QAing",
	"description": "QAing",
	"version": "1.0.0",
	"manifest_version": 3,
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},

	"action": {
		"default_title": "React Extension",
		"default_icon": "icon.png"
	},
	"host_permissions": [
		"http://localhost:3000/auth/google/callback",
		"https://app.qaing.co/auth/google/callback"
	],
	"permissions": ["storage", "context", "cookies", "activeTab", "scripting"],
	"options_page": "options.html",
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["contentScript.js"]
		}
	]
}
